// Creating instances of each derived class:

const singer = new Singer('Nayeon Im', 29, Genre.POP);
console.log(singer.getDescription());

const painter = new Painter('Leonardo da Vinci', 67, Medium.OIL);
console.log(painter.getDescription());

const writer = new Writer('J.K. Rowling', 58, WritingStyle.FICTION);
console.log(writer.getDescription());

const dancer = new Dancer('Misty Copeland', 41, DanceStyle.BALLET);
console.log(dancer.getDescription());
